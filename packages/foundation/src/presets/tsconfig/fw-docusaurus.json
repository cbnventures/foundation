{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/types",
      "@types/node",
      "@types/react",
      "@types/react-dom"
    ]
  }
}
